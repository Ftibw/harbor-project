    </update>
    <update id="updateByPrimaryKey" parameterType="com.whxm.harbor.bean.BizBuilding">
        update biz_building
        set number = #{number,jdbcType=VARCHAR},
            name   = #{name,jdbcType=VARCHAR},
            layer  = #{layer,jdbcType=INTEGER},
            type   = #{type,jdbcType=INTEGER},
            area   = #{area,jdbcType=CHAR},
            dx     = #{dx,jdbcType=DOUBLE},
            dy     = #{dy,jdbcType=DOUBLE}
        where id = #{id,jdbcType=INTEGER}
    </update>
    <!--以下时自定义部分-->
    <select id="getBuildingList" resultMap="BaseResultMap">
        SELECT
        <include refid="Base_Column_List"/>
        FROM biz_building
        <where>
            <if test="floor!=null">
                and layer=#{floor}
            </if>
            <if test="null!=typeList and typeList.size>0">
                and type in (
                <foreach collection="typeList" item="item" separator=",">
                    #{item}
                </foreach>
                )
            </if>
        </where>
    </select>

    <insert id="batchReplace" useGeneratedKeys="true" keyProperty="id">
        REPLACE INTO biz_building
        (<include refid="Base_Column_List"/>)
        VALUES
        <foreach collection="list" item="item" separator=",">
            (NULL , #{item.number,jdbcType=VARCHAR}, #{item.name,jdbcType=VARCHAR},
            #{item.layer,jdbcType=INTEGER}, #{item.type,jdbcType=INTEGER},
            #{item.area,jdbcType=CHAR}, #{item.dx,jdbcType=DOUBLE},
            #{item.dy,jdbcType=DOUBLE})
        </foreach>
    </insert>

    <delete id="batchDelete">
        DELETE from biz_building
        where id in (
        <foreach collection="list" item="item" separator=",">
            #{item}
        </foreach>
        )
    </delete>
