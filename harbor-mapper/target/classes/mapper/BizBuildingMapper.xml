        insert into biz_building (id, number, name, layer, type, area, dx, dy)
        values (#{id,jdbcType=INTEGER},
                #{number,jdbcType=VARCHAR},
                #{name,jdbcType=VARCHAR},
                #{layer,jdbcType=INTEGER},
                #{type,jdbcType=INTEGER},
                #{area,jdbcType=CHAR},
                #{dx,jdbcType=DOUBLE},
                #{dy,jdbcType=DOUBLE})
